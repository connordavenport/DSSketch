      <features copy="1"/>
      <info copy="1"/>
      <location>
        <dimension name="weight" xvalue="356"/>
        <dimension name="italic" xvalue="0"/>
      </location>
    </source>
    <source filename="SuperFont-sources/SuperFont_Black.ufo" name="source.3" familyname="SuperFont" stylename="Black">
